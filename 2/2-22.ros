(defun square (x)
  (* x x))

(defun square-list (items)
  "it's bug !"
    (labels
        ((iter (things answer)
           (if (null things)
               answer
               (iter (cdr things)
                     (cons (square (car things))
                           answer)))))
      (iter items nil)))

(defun square-list2 (items)
  "more bug !"
    (labels
        ((iter (things answer)
           (if (null things)
               answer
               (iter (cdr things)
                     (cons answer
                           (square (car things)))))))
      (iter items nil)))


